{
    "name": "your-name-here/CakeElements",
    "description": "CakeElements plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1",
        "knplabs/github-api": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeElements\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeElements\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
